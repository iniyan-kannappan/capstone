    "def fibo_sequence(n):\n",
    "    n1=1\n",
    "    n2=1\n",
    "    \n",
    "    for number in range(n):\n",
    "        seq=n1+n2\n",
    "        n1=n2\n",
    "        n2=seq\n",
    "\n",
    "        yield seq\n",
    "\n",
    "    "
   ]
  },
